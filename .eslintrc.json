{
	"env": {
		"es6": true,
		"browser": true
	},
	"globals": {},
	"extends": ["eslint:recommended", "prettier"],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"comma-dangle": ["error", "always"],
		"no-alert": "warn",
		"no-console": "warn",
		"no-unused-vars": "warn",
		"no-var": "warn",
		"eqeqeq": "warn"
	}
}
